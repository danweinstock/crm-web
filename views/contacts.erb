	<h1>All Contacts</h1>


	<ul>
				<li class="chng"><a href="/contacts/new">Add a contact</a></li>
        <li class="chng"><a href="/">Home</a></li>
     
  </ul>


		<ol>
			<% @contacts.each do |contact| %>
				<li><strong>First Name</strong>: <%= contact.first_name %>
				<strong>Last Name</strong>: <%= contact.last_name %>
				<strong>E-mail</strong>: <%= contact.email %>
				<strong>Note</strong>: <%= contact.note %></li>
				<button type="submit" class="btn">
					<h3><a href="/contacts/<%= contact.id %>/edit">Edit</a></h3>
				</button>
				<form action="/contacts/<%= contact.id %>" method="post" class="btn">
  					<input type="hidden" name="_method" value="delete">
 						 <input type="submit" value="Delete Contact">
				</form>
				<% end %>
			
		</ol>

			
